<% layout("/layouts/boilerplate") %>

<div class="container">
  <div class="row mt-5 justify-content-center">
    <div class="col-6">
      <div class="alert alert-danger alert-dismissible fade show" role="alert">
        <%= err.message %>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="alert"
          aria-label="Close"
        ></button>
      </div>
    </div>
  </div>
</div>
